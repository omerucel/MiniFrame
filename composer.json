{
    "name": "omerucel/miniframe",
    "type": "library",
    "require": {
        "php": ">=5.5.0",
        "symfony/http-foundation": "*",
        "zendframework/zend-config": "*",
        "symfony/console": "*",
        "monolog/monolog": "*",
        "doctrine/migrations": "dev-master",
        "doctrine/dbal": "*",
        "twig/twig": "*",
        "swiftmailer/swiftmailer": "*"
    },
    "require-dev": {
        "phacility/xhprof": "dev-master",
        "behat/behat": "*",
        "symfony/process": "*",
        "phpunit/phpunit": "*",
        "phpunit/dbunit": "*",
        "phpunit/php-code-coverage": "*"
    },
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "phacility/xhprof",
                "version":"dev-master",
                "source": {
                    "url": "git://github.com/phacility/xhprof.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "MiniFrame": "src/",
            "Application": "src/"
        }
    }
}